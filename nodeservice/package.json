{
  "name": "payment-agent",
  "version": "1.0.0",
  "description": "Local Node.js agent that launches .exe files triggered from Angular app",
  "main": "payment.js",
  "scripts": {
    "start": "node payment.js",
    "install-service": "node install-service.js"
  },
  "bin": "payment.js",
  "pkg": {
    "assets": [
      "install-service.js"
    ],
    "targets": [
      "node18-win-x64"
    ],
    "outputPath": "dist"
  },
  "author": "Alan Babu",
  "license": "MIT",
  "dependencies": {
    "cors": "^2.8.5",
    "express": "^4.18.2",
    "node-windows": "^1.0.0-beta.8"
  }
}
