	<logmode>rotate</logmode>
	<stoptimeout>30sec</stoptimeout>
	<serviceaccount>
		<domain>MSI</domain>
		<user>LocalSystem</user>
		<password></password>
	</serviceaccount>
	<workingdirectory>D:\Training\Angular\nodeProject\paymentthroughexe\nodeservice</workingdirectory>
